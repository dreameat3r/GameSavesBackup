<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="350" d:DesignHeight="175"
        x:Class="GameSavesBackup.SettingsWindow"
        Width="350"
        Height="175"
        CanResize="False"
        Background="#191919"
        Title="GameSavesBackup">

        <StackPanel>
                <TextBlock HorizontalAlignment="Center" Margin="0,10">Settings</TextBlock>
                <TextBlock HorizontalAlignment="Left" Margin="30, 5">Username: </TextBlock>
                <TextBox HorizontalAlignment="Left" Margin="20,0" Width="300"></TextBox>
                <Button Margin="10" HorizontalAlignment="Center" Width="90" HorizontalContentAlignment="Center">Save</Button>
        </StackPanel>
</Window>